{
    "name": "phpixie/bundles",
    "description": "Bundle library for PHPixie",
    "keywords": ["http", "psr-7"],
    "homepage": "http://phpixie.com",
    "type": "library",
    "license": "BSD",
    "authors": [
        {
            "name": "Roman Tsiupa",
            "email": "draconyster@gmail.com",
            "homepage": "http://dracony.org"
        }
    ],
    "require": {
        "phpixie/route": "3.*@dev",
        "phpixie/database": "3.*@dev",
        "phpixie/orm": "3.*@dev",
        "phpixie/filesystem": "3.*@dev",
        "phpixie/processors": "3.*@dev",
        "phpixie/slice": "3.*@dev",
        "phpixie/http": "3.*@dev",
        "psr/http-message": "1.0.*@dev"
    },
    "require-dev": {
        "phpixie/test": "3.*@dev"
    },
    "autoload": {
        "psr-4": {
            "PHPixie\\": "src/PHPixie",
            "PHPixie\\Tests\\": "tests/PHPixie/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
